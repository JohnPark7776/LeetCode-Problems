class Solution:
    """
    Runtime: 71 ms
    Memory: 14 MB
    """
    def addTwoNumbers(self, l1: Optional[ListNode], l2: Optional[ListNode]) -> Optional[ListNode]:
        num = 0
        final = l3 = ListNode()
        while l1 or l2:
            if l1 != None:
                num += l1.val
                l1 = l1.next
            if l2 != None:
                num += l2.val
                l2 = l2.next
            l3.next = ListNode(num%10)
            l3 = l3.next
            num //= 10
        if num != 0:
            l3.next = ListNode(num)
        return final.next
